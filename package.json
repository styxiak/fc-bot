{
    "name": "fc-bot",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "prebuild": "rm build/* -Rf",
        "build": " tsc ",
        "postbuild": "cp package.json build/package.json && cp .env* build/ && cd build && npm install --only=production",
        "start": "node main.js",
        "start-dev": "NODE_ENV=dev node main.js",
        "dev": "NODE_ENV=dev nodemon main.js",
        "test": "mocha -r ts-node/register tests/**/*.test.ts",
        "coverage": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test"
    },
    "dependencies": {
        "@typeit/discord": "^1.0.12",
        "@types/mysql": "^2.15.17",
        "@types/nconf": "^0.10.0",
        "@types/node": "^13.13.41",
        "@types/node-persist": "^3.1.1",
        "@types/table": "^6.0.0",
        "@types/ws": "^7.4.0",
        "command-line-args": "^5.1.1",
        "discord.js": "^12.5.1",
        "dotenv": "^8.2.0",
        "emoji-strip": "^1.0.1",
        "human-readable-numbers": "^0.9.5",
        "moment": "^2.29.2",
        "mysql": "^2.18.1",
        "nconf": "^0.10.0",
        "node-cron": "^2.0.3",
        "node-persist": "^3.1.0",
        "novelcovid": "^1.2.7",
        "reflect-metadata": "^0.1.13",
        "table": "^6.0.7",
        "text-table": "^0.2.0",
        "tslib": "^1.14.1"
    },
    "devDependencies": {
        "@types/chai": "^4.2.15",
        "@types/mocha": "^8.2.1",
        "chai": "^4.3.1",
        "mocha": "^8.3.0",
        "nodemon": "^1.18.9",
        "nyc": "^15.1.0",
        "ts-node": "^9.1.1",
        "typescript": "^3.9.9"
    }
}
